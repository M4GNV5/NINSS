<?xml version="1.0" encoding="UTF-8"?>
<root>
  <!-- Server.jar file. %auto% only works with one file!-->
  <ServerFile>%auto%</ServerFile>
  <!--Java installation path. Use java.exe (for other version/64 bit use the full path) for default under windows and java under linux/mac-->
  <JavaExecutable>java.exe</JavaExecutable>
  <!-- Java arguments type java in console for help with arguments!-->
  <JavaArguments>-jar %jar% nogui</JavaArguments>
  <!-- Wheter Plugns are enabled or not. Default: true, NINSS doesnt make much sense without plugins-->
  <EnablePlugins>true</EnablePlugins>
  <!-- If true you wont be able to read much Console output after serverstop -->
  <Close_Window_on_Serverstop>true</Close_Window_on_Serverstop>
</root>